alias ..="cd .."
alias ...="cd ../.."

alias h='cd ~'
alias c='clear'

function cakebox() {
  sudo /usr/bin/php -q /cakebox/cakebox.php $@
}

function cakebox-app() {
  sudo bash /cakebox/cakebox-app.sh $@
}

function cakebox-database() {
  sudo bash /cakebox/cakebox-database.sh $@
}

function cakebox-package() {
  sudo bash /cakebox/cakebox-package.sh $@
}

function cakebox-site() {
  sudo bash /cakebox/cakebox-site.sh $@
}
