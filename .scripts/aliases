alias ..="cd .."
alias ...="cd ../.."

alias h='cd ~'
alias c='clear'

function cakebox-app() {
  sudo bash /cakebox/cakebox-app.sh "$1" "$2"
}

function cakebox-database() {
  sudo bash /cakebox/cakebox-database.sh "$1"
}

function cakebox-package() {
  sudo bash /cakebox/cakebox-package.sh "$1"
}

function cakebox-site() {
  sudo bash /cakebox/cakebox-site.sh "$1" "$2"
}
